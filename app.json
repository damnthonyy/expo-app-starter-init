{
  "expo": {
    "name": "learn-expo",
    "slug": "learn-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/picmecarton.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/picmecarton.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/picmecarton.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
